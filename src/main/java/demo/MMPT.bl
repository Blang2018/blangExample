package demo

model MMPT {
  random RealVar x 
  random RealVar dummy 
  
  laws {
  	logf(x,dummy)  {
      Math.log(Math.exp(-0.5 * pow(2-x, 2)) + Math.exp(-0.5 * pow(-2-x, 2)))
    }
  }
  
  generate(rand){
